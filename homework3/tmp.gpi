\
set terminal png;\
set output "least.pyxplot.png";\
set key top right;\
set tics out;\
set xlabel "Time (days)";\
set ylabel "Intensity";\
set title "Least squares";\
plot \
"datafile.txt" using 1:2 with lines title "Our fit"\
,"datafile.txt" using 1:3 with lines title "Our fit (upsum)"\
,"datafile.txt" using 1:4 with lines title "Our fit (downsum)"\
    ,"data.txt" using 1:4:5 with yerrorbars title "Data with errorbars"\

